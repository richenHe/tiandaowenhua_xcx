{
  "mcpServers": {
    "tdesign-mcp-server": {
      "command": "npx",
      "args": [
        "-y",
        "tdesign-mcp-server"
      ]
    },
    "cloudbase": {
      "command": "npx",
      "args": [
        "npm-global-exec@latest",
        "@cloudbase/cloudbase-mcp@latest"
      ],
      "env": {
        "INTEGRATION_IDE": "Cursor"
      }
    }
  }
}
